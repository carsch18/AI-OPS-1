# AIOps Platform Environment Configuration
# Copy this file to .env and fill in your values

# Required: Cerebras API Key
# Get yours at: https://cloud.cerebras.ai
CEREBRAS_API_KEY=your_api_key_here

# Optional: Netdata URL (default: http://localhost:19999)
NETDATA_URL=http://localhost:19999

# Optional: PostgreSQL Database URL
DATABASE_URL=postgresql://aiops:aiops_password@localhost:5432/aiops_brain

# Optional: Ansible EDA Webhook URL
ANSIBLE_EDA_URL=http://localhost:5000
